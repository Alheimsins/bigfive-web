{
  "name": "bigfive-web",
  "version": "2.0.1",
  "description": "Website for five factor model of personality",
  "private": true,
  "author": {
    "name": "Peter DePaulo",
    "email": "peter.depaulo@sophonaut.com",
    "url": "https://github.com/Sophonaut"
  },
  "contributors": [
    {
      "name": "Jonas Enge",
      "email": "jonas.enge@t-fk.no",
      "url": "https://github.com/telemark"
    },
    {
      "name": "Geir GÃ¥sodden",
      "email": "geir.gasodden@pythonia.no",
      "url": "https://github.com/zrrrzzt"
    }
  ],
  "main": "server.js",
  "engines": {
    "node": ">=12.16.1"
  },
  "scripts": {
    "test": "standard && ava",
    "audit": "npm audit",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "node backend/server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "cleanup": "rm -rf .next",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "deploy": "now && now alias"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sophonaut/bigfive-web.git"
  },
  "keywords": [
    "bigfive",
    "personality"
  ],
  "license": "MIT",
  "dependencies": {
    "@alheimsins/b5-johnson-120-ipip-neo-pi-r": "Sophonaut/b5-johnson-120-ipip-neo-pi-r#main",
    "@alheimsins/react-google-charts": "1.6.5",
    "@sophonaut/b5-result-text": "github:Sophonaut/b5-result-text#main",
    "@stripe/stripe-js": "^1.15.0",
    "axios": "^0.21.1",
    "b5-calculate-score": "1.0.4",
    "babel-eslint": "10.1.0",
    "bcrypt": "^5.0.1",
    "chart.js": "^2.9.4",
    "connect-mongo": "^4.4.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.2",
    "firebase": "^8.6.3",
    "helmet": "3.23.3",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "3.1.1",
    "i18next-express-middleware": "2.0.0",
    "i18next-node-fs-backend": "2.1.3",
    "i18next-xhr-backend": "1.5.1",
    "iso-639-1": "2.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongojs": "3.1.0",
    "mongoose": "^5.12.14",
    "mongoose-unique-validator": "^2.0.3",
    "next": "^10.2.3",
    "next-offline": "^5.0.3",
    "next-routes": "1.4.2",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "react": "16.13.1",
    "react-chartjs-2": "^2.11.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.13.1",
    "react-google-charts": "3.0.15",
    "react-i18next": "^9.0.10",
    "react-icons": "3.11.0",
    "react-router-dom": "^5.2.0",
    "request-country": "0.1.5",
    "stripe": "^8.154.0"
  },
  "devDependencies": {
    "ava": "3.13.0",
    "coveralls": "3.1.0",
    "husky": "4.3.0",
    "nyc": "15.1.0",
    "standard": "14.3.4"
  },
  "standard": {
    "globals": [
      "next",
      "fetch",
      "data"
    ],
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "ava": {
    "files": [
      "test/**/*"
    ]
  }
}
